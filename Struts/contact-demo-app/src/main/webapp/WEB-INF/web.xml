<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>welcome-gradle</display-name>
	<welcome-file-list>
		<welcome-file>home</welcome-file>
	</welcome-file-list>
	<filter>
		<filter-name>getContact</filter-name>
		<filter-class>com.techlabs.filter.GetContact</filter-class>
	</filter>
	<filter>
		<filter-name>SetContact</filter-name>
		<filter-class>com.techlabs.filter.SetContact</filter-class>
	</filter>
	<filter>
		<filter-name>LoginFilter</filter-name>
		<filter-class>com.techlabs.filter.LoginFilter</filter-class>
	</filter>
	<filter>
		<filter-name>SessionCheck</filter-name>
		<filter-class>com.techlabs.filter.SessionCheck</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>getContact</filter-name>
		<url-pattern>/edit</url-pattern>
		<url-pattern>/deleteCtrl</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>SetContact</filter-name>
		<url-pattern>/addCtrl</url-pattern>
		<url-pattern>/editCtrl</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/loginCtrl</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>SessionCheck</filter-name>
		<url-pattern>/home</url-pattern>
		<url-pattern>/add</url-pattern>
		<url-pattern>/edit</url-pattern>
	</filter-mapping>
	
	<servlet>
		<servlet-name>HomeController</servlet-name>
		<servlet-class>com.techlabs.controller.HomeController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomeController</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EditController</servlet-name>
		<servlet-class>com.techlabs.controller.EditController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditController</servlet-name>
		<url-pattern>/editCtrl</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddController</servlet-name>
		<servlet-class>com.techlabs.controller.AddController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddController</servlet-name>
		<url-pattern>/addCtrl</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DeleteController</servlet-name>
		<servlet-class>com.techlabs.controller.DeleteController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteController</servlet-name>
		<url-pattern>/deleteCtrl</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>com.techlabs.controller.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/loginCtrl</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LogoutController</servlet-name>
		<servlet-class>com.techlabs.controller.LogoutController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutController</servlet-name>
		<url-pattern>/logoutCtrl</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Edit</servlet-name>
		<jsp-file>/edit.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Edit</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Login</servlet-name>
		<jsp-file>/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Add</servlet-name>
		<jsp-file>/add.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Add</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

</web-app>